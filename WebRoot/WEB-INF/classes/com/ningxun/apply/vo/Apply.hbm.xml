<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        '-//Hibernate/Hibernate Mapping DTD 3.0//EN'
        'http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd'>
<hibernate-mapping>
    <class name="com.ningxun.apply.vo.Apply" table="apply">
        <id name="id">
            <generator class="native" />
        </id>
        <property name="uid" column="uid"/>
        <property name="tid" column="tid"/>
        <property name="applyHandle" column="applyHandle"/>
        <property name="applyTime" column="applyTime"/>
        <property name="handleTime" column="handleTime"/>
        <property name="handleUser" column="handleUser"/>
        <property name="reason" column="reason"/>
    </class>
</hibernate-mapping>
