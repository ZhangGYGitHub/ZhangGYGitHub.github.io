<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        '-//Hibernate/Hibernate Mapping DTD 3.0//EN'
        'http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd'>
<hibernate-mapping>
    <class name="com.ningxun.clothes.vo.Clothes" table="clothes">
        <id name="id">
            <generator class="native" />
        </id>
        <property name="tid" column="tid"/>
        <property name="clothesColour" column="clothesColour"/>
        <property name="clothesPicNewName" column="clothesPicNewName"/>
        <property name="clothesPicOldName" column="clothesPicOldName"/>
    </class>
</hibernate-mapping>
